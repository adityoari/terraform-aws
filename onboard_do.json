{
    "schemaVersion": "1.0.0",
    "class": "Device",
    "async": true,
    "label": "my BIG-IP declaration for declarative onboarding",
    "Common": {
        "class": "Tenant",
        "hostname": "tfaws1.example.com",
        "myDns": {
            "class": "DNS",
            "nameServers": [
                "8.8.8.8"
            ]
        },
        "myNtp": {
            "class": "NTP",
            "servers": [
                "0.pool.ntp.org"
            ],
            "timezone": "Asia/Singapore"
        },
        "admin": {
            "class": "User",
            "userType": "regular",
            "password": "{{{ ADMIN_PASS }}}",
            "shell": "bash",
            "keys": ["{{{ SSH_KEYS }}}"]
        },
        "{{{ USER_NAME }}}": {
            "class": "User",
            "userType": "regular",
            "password": "{{{ ADMIN_PASS }}}",
            "shell": "bash",
            "keys": ["{{{ SSH_KEYS }}}"],
            "partitionAccess": {
                "all-partitions": {
                    "role": "admin"
                }
            }
        },
	"configsync": {
            "class": "ConfigSync",
            "configsyncIp": "{{ SELF_INTERNAL }}"
        },
        "failoverAddress": {
            "class": "FailoverUnicast",
            "address": "{{ SELF_INTERNAL }}"
        },
        "failoverGroup": {
            "class": "DeviceGroup",
            "type": "sync-failover",
            "members": [
                "tfaws1.example.com"
            ],
            "owner": "/Common/failoverGroup/members/0",
            "autoSync": true,
            "saveOnAutoSync": false,
            "networkFailover": true,
            "fullLoadOnSync": false,
            "asmSync": false
	}
    }
}
